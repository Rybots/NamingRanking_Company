<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>順位</th>
      <th>名前</th>
      <th>ネーミングスコア</th>
      <th>上場企業順位</th>
    </tr>
  </thead>
  <tbody>
  <% @scores.each do |score| %>
    <tr>
      <td><%= score.id %></td>
      <td><%= score.name %></td>
      <td><%= score.score %></td>
      <td><%= score.ranking %></td>
    </tr>
  <% end %>
</tbody>
</table>
